<p id="notice"><%= notice %></p>

<h1>Snippet Uploads</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @snippet_uploads.each do |snippet_upload| %>
      <tr>
        <td><%= snippet_upload.title %></td>
        <% if snippet_upload.user !=nil %> 
          <td><%= snippet_upload.user.email %></td>
        <% else %>
          <td> none </td>
        <% end %>
        <td><%= link_to 'Show', snippet_upload %></td>
        <td><%= link_to 'Edit', edit_snippet_upload_path(snippet_upload) %></td>
        <td><%= link_to 'Destroy', snippet_upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Snippet Upload', new_snippet_upload_path %>
